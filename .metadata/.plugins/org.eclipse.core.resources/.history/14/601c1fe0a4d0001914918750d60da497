package test_96;

import java.util.List;
public class Solution {
	public static void main(String[] args) {
    String s="900 google.mail.com";
    String[] t=s.split(" ");
    for(String a:t) {
    	System.out.println(a);
    }
    int n=Integer.valueOf(t[0]);
    String[] v= {"google","com"};
    String w=String.join(".", v);
    System.out.println(w);
	}
}
