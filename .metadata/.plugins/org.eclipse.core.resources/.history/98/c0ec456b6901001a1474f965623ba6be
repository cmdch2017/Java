package test117;

public class Main2 {
	public static void main(String[] args) {
		String s1="abcd12345ed125ss123456789";
		char[] array=s1.toCharArray();
		int maxCount=0;
		int count=0;
		for(int i=0;i<array.length;i++) {	
			if(array[i]<'0'||array[i]>'9') {
				count=0;
			}
			else {
				count++;
				if(maxCount<count) {
					maxCount=count;
				}
			}
		}
		System.out.println(count+" "+maxCount);
	}
}
