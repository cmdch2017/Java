package 抽象类;
interface Animal{
	String AnimalBehavior="动物的行为";
	void breathe();//定义抽象方法
	void run();
}
class Dog implements Animal{
public void breathe() {
	System.out.println(AnimalBehavior+"："+"狗在呼吸");
}
public void run() {
	System.out.println(AnimalBehavior+":"+"狗在奔跑");
}
}
public class Solution {
 public static void main(String[] args) {
	 Dog dog=new Dog();
	 dog.breathe();
	 dog.run();
 }
}
